module.exports = (variables) => {
    require('dotenv').config();
    if(!variables.Discord) variables.Discord = require('discord.js');
    if(!variables.MessageEmbed) variables.MessageEmbed = variables.Discord.MessageEmbed;
    if (!variables.MessageAttachment) variables.MessageAttachment = variables.Discord.MessageAttachment;
    if (!variables.prettysize) variables.prettysize = require('prettysize');
    if (!variables.fetch) variables.fetch = require('node-fetch');
    let Intents = variables.Discord.Intents;
    if (!variables.moment) variables.moment = require('moment');
    variables.moment.locale('fr');
    if(!variables.client) variables.client = variables.bot = new variables.Discord.Client({disableEveryone: true, ws: { intents: new Intents(Intents.ALL) }});
    if(!variables.fs) variables.fs = require('fs');
    if(!variables.getFiles) variables.getFiles = (dir, files_) => {
      files_ = files_ || [];
      if(variables.fs.existsSync(dir)) {
        var files = variables.fs.readdirSync(dir);
        for(var i in files) {
          var name = dir + '/' + files[i];
          if(variables.fs.statSync(name).isDirectory()) {
            variables.getFiles(name, files_);
          } else {
            files_.push(name);
          };
        };
      };
      return files_;
    };
    if(!variables.listeners) variables.listeners = [];
  
    variables.colors = { default: 0x00D8EC, success: 0x076A00, warning: 0xFF7400, error: 0xBC0101, unknown: 0x686868 };
    variables.version = 'v1.0.0';
    variables.downtime = { status: false, reason: null };
    variables.owners = ['680885379431137334', '848618744045109318'];
    variables.permissionsList = { ADMINISTRATOR: 'Administrateur', CREATE_INSTANT_INVITE: 'Créer une invitation', KICK_MEMBERS: 'Expulser des membres', BAN_MEMBERS: 'Bannir des membres', MANAGE_CHANNELS: 'Gérer les salons', MANAGE_GUILD: 'Gérer le serveur', ADD_REACTIONS: 'Ajouter des récations', VIEW_AUDIT_LOG: 'Voir les logs du serveur', PRIORITY_SPEAKER: 'Voix prioritaire', STREAM: 'Vidéo', VIEW_CHANNEL: 'Voir le salon', SEND_MESSAGES: 'Envoyer des messages', SEND_TTS_MESSAGES: 'Envoyer des messages TTS', MANAGE_MESSAGES: 'Gérer les messages', EMBED_LINKS: 'Intégrer des liens', ATTACH_FILES: 'Joindre des fichiers', READ_MESSAGE_HISTORY: 'Voir l\'historique des messages', MENTION_EVERYONE: 'Mentionner everyone, here et tous les rôles', USE_EXTERNAL_EMOJIS: 'Utiliser des emojis externes', VIEW_GUILD_INSIGHTS: 'Voir les analyses du serveur', CONNECT: 'Se connecter', SPEAK: 'Parler', MUTE_MEMBERS: 'Couper le micro des membres', DEAFEN_MEMBERS: 'Mettre en sourdine des membres', MOVE_MEMBERS: 'Déplacer des membres', USE_VAD: 'Utiliser la Détection de la voix', CHANGE_NICKNAME: 'Changer le pseudo', MANAGE_NICKNAMES: 'Gérer les pseudos', MANAGE_ROLES: 'Gérer les rôles', MANAGE_WEBHOOKS: 'Gérer les webhooks', MANAGE_EMOJIS: 'Gérer les emojis' };
  }
